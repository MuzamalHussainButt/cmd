ğŸ¤—ï¸create mutation
mutation createorder($input: CreateOrderMutationInput!){
  createOrder(input: $input){
    order{
      id
      signatureUrl
      buyerId
      note
      reference
      hold
      userFullName
    }
  }
}
{
  "input": {
    "catalogId":1,
    "attributes": {
      "note": "aaaaaaa44aaa",
      "hold": true,
      "category": "aaaa444aaaaaa",
      "tos": "aaaaa",
      "lines": {
          "quantity": 1,
          "productId": 1,
          "varietyId": 1
      }
    }
  }
}

ğŸ¤—ï¸destroy mutation
mutation DestroyOrder($input: DestroyOrderMutationInput!){
  destroyOrder(input: $input){
    errors {
            code
            message
            path
          }
    success
    
   
  }
}

